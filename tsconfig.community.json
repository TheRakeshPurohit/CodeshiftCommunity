{
  "compilerOptions": {
    "composite": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["community/**/*", "scripts"],
  "exclude": [
    "node_modules",
    "plugin_packages",
    "dist",
    "__tests__",
    "**/*.test*",
    "**/*.spec*"
  ]
}
